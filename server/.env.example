# LLM API Keys（至少填一个，Agent 才能说话）
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
OPENROUTER_API_KEY=

# 自定义API（OpenAI 兼容）
# 支持完整 endpoint（例如：https://api.siliconflow.cn/v1/chat/completions）
CUSTOM_API_KEY=
CUSTOM_API_BASE_URL=https://api.siliconflow.cn/v1/chat/completions
CUSTOM_API_MODEL=

# 硅基流动 Embedding API（免费注册 https://siliconflow.cn）
# 用于记忆系统的语义搜索，不填则记忆搜索不可用
# 推荐使用 EMBEDDING_API_KEY（仍兼容 SILICONFLOW_API_KEY）
EMBEDDING_API_KEY=

# Debug
DEBUG=true

# === Embedding 模型（本地模型路径，可选） ===
# EMBEDDING_MODEL_PATH=E:/bge-small-zh-v1.5
